

 
 <div class="pusher">

<div id ="showinstructor">
<% if current_user && current_user.admin %>
<% @cohorts.each do |coho| %>
    <div id ="cohort-<%=coho.id%>">
    <h1> <%= coho.name %></h1>
    <h4><%= link_to 'Show', coho %></h4>
        <h4><%= link_to 'Edit', edit_cohort_path(coho) %></h4>
        <%= link_to 'Destroy', cohort_path(coho), method: :delete, remote:true, data: { confirm: 'Are you sure?' } %>
        </div>
       
    <%end%>

  <%else%>
  <h1> <%= @cohorts.name %></h1>
  <h4><%= link_to 'Show', @cohorts %></h4>
  <%end%>
</div>
 </div>