<div class="ui form">
<%= form_for @cohorts do |f| %>
    
    <div class="field">
      <% f.label :name %>Name: 
      <%= f.text_field :name %>
    </div>
  
    <div class="field">
      <% f.label :start_hours%> Star date: 
      <%= f.date_field :start_date %>
    </div>

    <div class="field">
    <% f.label :end_hours%> End date: 
    <%= f.date_field :end_date %>
  </div>

  <div class="field">
  <% f.label :instructor_id%> Instructor: 
  <%= f.select(:instructor_id) do %>
                <% @instructors.order('last_name ASC').each do |instructor_select| %>
                  <%= content_tag(:option, instructor_select.last_name, value: instructor_select.id) %>
                <% end %>
            <% end %>
</div>
  
<div class="field">
<% f.label :course_id%> Course: 
<%= f.select(:course_id) do %>
                <% @courses.order('name ASC').each do |course_select| %>
                  <%= content_tag(:option, course_select.name, value: course_select.id) %>
                <% end %>
            <% end %>
</div>
    
    <div class="btn">
    <div class="actions">
    <%= f.submit "Submit", :class => "ui positive button active"%>
      
    </div>
    <divn class="ui button">
    <%= link_to 'Back', '/cohorts' %>
    </div>
    </div>
    </div>
  <% end %>

 